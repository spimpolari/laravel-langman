{
    "name": "spimpolari/laravel-langman",
    "description": "Manage language files with ease.",
    "keywords": ["laravel", "localization", "multilingual"],
    "homepage": "https://github.com/themsaid/laravel-langman",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefano Pimpolari",
            "email": "stefano.pimpolari@gmail.com",
            "homepage": "http://devguru.it"
        }
    ],
    "require": {
        "php": "^7.1 || ^7.2 || ^7.3",
        "laravel/framework": "6.* || 7.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0 || ^8.0",
        "orchestra/testbench": "~4.0",
        "orchestra/database": "~4.0",
        "mockery/mockery": "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "Spimpolari\\Langman\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spimpolari\\Langman\\LangmanServiceProvider"
            ]
        }
    }
}
