{
    "name": "ttensrl/laravel-langman",
    "description": "Manage language files with ease.",
    "keywords": ["laravel", "localization", "multilingual"],
    "homepage": "https://github.com/spimpolari/laravel-langman",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefano Pimpolari",
            "email": "stefano.pimpolari@gmail.com",
            "homepage": "http://devguru.it"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"

    },
    "require-dev": {
        "phpunit/phpunit" : "^10.4",
        "orchestra/testbench": "~8.0",
        "orchestra/database": "~6.0",
        "mockery/mockery": "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "spimpolari\\Langman\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spimpolari\\Langman\\LangmanServiceProvider"
            ]
        }
    }
}
